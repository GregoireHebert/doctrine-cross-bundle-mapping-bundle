{
    "name": "gheb/doctrine-cross-bundle-mapping-bundle",
    "description": "This Bundle brings cross bundle mapping",
    "keywords": ["doctrine", "cross", "bundle", "mapping"],
    "homepage": "https://github.com/GregoireHebert/DoctrineCrossBundleMappingBundle",
    "license": "MIT",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Grégoire Hébert",
            "email": "gregoire59166@gmail.com",
            "role": "Developer"
        }
    ],

    "support": {
        "email": "gregoirehebert@reflexece.com",
        "issues": "https://github.com/GregoireHebert/DoctrineCrossBundleMappingBundle/issues",
        "source": "https://github.com/GregoireHebert/DoctrineCrossBundleMappingBundle"
    },

    "autoload":     {
        "psr-4": { "Gheb\\Bundle\\DoctrineCrossBundleMappingBundle\\": "" }
    },

    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.7",
        "doctrine/common": "2.5.*",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",
        "knplabs/doctrine-behaviors": "1.0.*@dev"
    },

    "require-dev": {
        "phpunit/phpunit": "4.4.*"
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "config": {
        "bin-dir": "bin"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
